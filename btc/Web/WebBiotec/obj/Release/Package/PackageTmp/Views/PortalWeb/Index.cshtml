<script>

    $(document).ready(function () {

        kendo.culture("es-CL");
        // create DatePicker from input HTML element
        $("#datepicker").kendoDatePicker();

        $("#monthpicker").kendoDatePicker({
            // defines the start view
            start: "year",

            // defines when the calendar should return date
            depth: "year",

            // display month and year in the input
            //format: "MMMM yyyy",
            format: "MM-yyyy",
            maxDate: '0',

            // specifies that DateInput is used for masking the input element
            dateInput: true,
            animation: {
                close: {
                    effects: "zoom:out",
                    duration: 300
                }
            }

        });

        var clientesDS = new kendo.data.DataSource({
            transport: {
                read: {
                    url: '@Url.Action("cargarClientes", "PortalWeb")',
                    dataType: "json",
                    //data: {
                    //    cod: "ZONASLOCALES",
                    //    idcanal: canal,
                    //    idzona: zona,
                    //    idregion: region,
                    //    iddistribuidor: distribuidor
                    //}
                }
            },
            schema: {
                model: {
                    fields: {
                        valor: { type: "string" },
                        texto: { type: "string" }
                    }
                }
            }
        });

        $("#comboboxClientes").kendoComboBox({
            //placeholder: "Todos",
            clearButton: false,
            dataValueField: "valor",
            dataTextField: "texto",
            dataSource: clientesDS,
            animation: {
                close: {
                    effects: "zoom:out",
                    duration: 300
                }
            }
        });

        var today = new Date();
        var dd = today.getDate();
        var mm = today.getMonth();
        var yyyy = today.getFullYear();

        var sumaMes = 1;
        var fechaActual;

        if (mm == 12) {
            fechaActual = 1 + '-' + yyyy;
        }
        else {
            fechaActual = mm + 1 + '-' + yyyy;
        }
        
        $("#monthpicker").data('kendoDatePicker').value(fechaActual);
        //$("#monthpicker").data('kendoDatePicker').value('09-2017');

        var documentosDS = new kendo.data.DataSource({
            transport: {
                read: {
                    url: '@Url.Action("cargarClientes", "PortalWeb")',
                    dataType: "json",
                }
            },
            schema: {
                model: {
                    fields: {
                        valor: { type: "string" },
                        texto: { type: "string" }
                    }
                }
            }
        });

        $("#comboboxNumeroDocumento").kendoComboBox({
            //placeholder: "Todos",
            clearButton: false,
            dataValueField: "valor",
            dataTextField: "texto",
            dataSource: documentosDS,
            animation: {
                close: {
                    effects: "zoom:out",
                    duration: 300
                }
            }
        });

    });

</script>

